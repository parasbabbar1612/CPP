#include<map>
#include<conio.h>
#include<iostream>

using namespace std;

int main()
{
	map<char, int> m;
	string s = "mississippi";
	for (int i = 0; i < s.length(); i++) m[s[i]]++;
	map<char, int>::iterator it;
	for (it = m.begin(); it != m.end(); it++)
	{
		cout << it->first << " " << it->second<<endl;
	}

}